<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="100dp">

    <TextView
        android:id="@+id/measuredProofMeasurementListItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Measured Proof: 100.1"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="7dp" />

    <TextView
        android:id="@+id/measuredAtTimeMeasurementListItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Five minutes ago"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="8dp" />

    <TextView
        android:id="@+id/measuredAtDateMeasurementListItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="11/11/11"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="41dp" />
</androidx.constraintlayout.widget.ConstraintLayout>